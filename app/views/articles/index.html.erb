<div class="index content">
  <div class="logo">
    <img id="title-logo" src="https://vectr.com/jaybur/gsFRFWZNG.svg?width=640&height=200&select=gsFRFWZNGpage0" alt="cobungo-logo">
  </div>
  <h1>All Articles</h1>
  <ul id="articles">
    <% @articles.each do |article| %>
      <li>
        <%= link_to article.title, article_path(article), class: 'article_title' %>
      </li>
    <% end %>
  </ul>
  <%= link_to "Create a new Article", new_article_path, class: 'new_article action_button' %>
</div>
