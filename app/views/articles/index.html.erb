<div class="index content">
  <h1>All Articles</h1>
  <ul id="articles">
    <% @articles.each do |article| %>
      <li>
        <%= link_to article.title, article_path(article), class: 'article_title' %>
      </li>
    <% end %>
  </ul>
  <%= link_to "Create a new Article", new_article_path, class: 'new_article action_button' %>
</div>
<p class='footer'>Coded by JayBur&copy;2019</p>
<div class="logo">
  <%=svg 'title_logo' %>
</div>
